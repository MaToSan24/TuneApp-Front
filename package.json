{
  "name": "tuneapp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint",
    "test": "mocha tests/unit/*.js --timeout 15000",
    "ci": "start-test \"npm run serve\" 8080 test"
  },
  "dependencies": {
    "@vue/cli": "^4.5.13",
    "abcjs": "^5.12.0",
    "axios": "^0.24.0",
    "body-parser": "^1.19.1",
    "chai-http": "^4.3.0",
    "chart.js": "3.3.2",
    "cookie-parser": "^1.4.6",
    "core-js": "^3.6.5",
    "dotenv": "^10.0.0",
    "jsonwebtoken": "^8.5.1",
    "mocha": "^9.1.3",
    "primeflex": "3.0.0",
    "primeicons": "5.0.0",
    "primevue": "3.9.1",
    "prismjs": "1.9.0",
    "vue": "^3.0.0",
    "vue-axios": "^3.4.0",
    "vue-router": "^4.0.0-0",
    "vue3-ace-editor": "^2.2.2",
    "vuex": "^4.0.0-0",
    "vuex-persistedstate": "^4.1.0",
    "font-awesome": "^4.7.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-unit-mocha": "^4.5.15",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "@vue/test-utils": "^2.0.0-0",
    "babel-eslint": "^10.1.0",
    "chai": "^4.3.4",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^7.0.0",
    "node-sass": "^4.12.0",
    "sass-loader": "^8.0.2",
    "start-server-and-test": "^1.14.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {
      "no-unused-vars": "off"
    },
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "mocha": true
        }
      }
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
